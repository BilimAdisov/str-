#test test_s21_strrchr   
ck_assert_str_eq(s21_strrchr("Hello, World!", 'o'), strrchr("Hello, World!", 'o'));
ck_assert_ptr_eq(s21_strrchr("Hello, World!", 'x'), strrchr("Hello, World!", 'x'));
ck_assert_ptr_eq(s21_strrchr(s21_NULL, 'W'), s21_NULL);